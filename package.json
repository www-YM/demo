{
  "name": "thefront-ts--next",
  "author": "maccarian agency",
  "email": "hi@maccarianagency.com",
  "version": "3.2.2",
  "private": true,
  "scripts": {
    "start:dev": "next dev -p 3005",
    "start": "next start",
    "build": "cross-env GENERATE_SOURCEMAP=false next build",
    "prettier": "prettier --write '**/*.{js,css,html}'",
    "clear-all": "rimraf ./.cache ./.next ./node_modules",
    "prepare": "husky install",
    "extract:i18n": "formatjs extract '{pages,src}/**/*.{js,ts,tsx}' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/en.json",
    "compile:i18n": "formatjs compile-folder --ast content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n"
  },
  "browser": {
    "child_process": false
  },
  "eslintConfig": {
    "plugins": [
      "formatjs"
    ],
    "rules": {
      "formatjs/no-offset": "error"
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
